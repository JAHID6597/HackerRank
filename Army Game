#include<bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    int n,m;
    cin>>n>>m;
    if(n%2==0 && m%2==0)cout<<(n*m)/4<<endl;
    else if(n%2==1 && m%2==1)cout<<( ((n-1)*(m-1))/4+(n+m-1)/2 + 1  )<<endl;
    else if(n%2==0 && m%2==1)cout<<(n*(m-1))/4+n/2<<endl;
    else if(n%2==1 && m%2==0)cout<<((n-1)*m)/4+m/2<<endl;
    return 0;
}
