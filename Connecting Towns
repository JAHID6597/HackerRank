#include <bits/stdc++.h>
using namespace std;

#define mod 1234567

int main()
{
    int t;
    cin>>t;
    while (t--) {
        int n,a=1;
        cin>>n;
        for(int i=0;i<n-1;i++){
            int x;
            cin>>x;
            a = a * x;
            a = a % mod;
        }
        cout<<a<<endl;
    }
    return 0;
}
